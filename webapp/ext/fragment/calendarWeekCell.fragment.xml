<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:smartField="sap.ui.comp.smartfield" xmlns:l="sap.ui.layout"
	xmlns:viz.data="sap.viz.ui5.data" xmlns:viz="sap.viz.ui5.controls" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/10" xmlns:micro="sap.suite.ui.microchart">
	

<viz:VizFrame id="idVizFrameHeatmap" vizType="heatmap" width="100%" height="100%" uiConfig="{applicationSet:'fiori'}">
    <viz:dataset>
        <viz.data:FlattenedDataset data="{ path: 'to_heatmap'}">
            <viz.data:dimensions>
                <viz.data:DimensionDefinition name="Row" value="{YAchse}"/>
                <viz.data:DimensionDefinition name="Column" value="{XAchse}"/>
            </viz.data:dimensions>
            <viz.data:measures>
                <viz.data:MeasureDefinition name="Value" value="{value}"/>
            </viz.data:measures>
        </viz.data:FlattenedDataset>
    </viz:dataset>
    <viz:feeds>
        <viz.feeds:FeedItem uid="color" type="Measure" values="{Renevue}"/>
        <viz.feeds:FeedItem uid="categoryAxis" type="Dimension" values="XAchse"/>
        <viz.feeds:FeedItem uid="categoryAxis2" type="Dimension" values="YAchse"/>
    </viz:feeds>
</viz:VizFrame>




